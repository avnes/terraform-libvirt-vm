{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "branchPrefix": "feature/renovate/",
    "dryRun": false,
    "username": "avnes",
    "onboarding": false,
    "platform": "github",
    "repositories": [
        "avnes/valyria-vm"
    ],
    "packageRules": [{
        "matchUpdateTypes": [
            "pin",
            "digest",
            "patch",
            "minor",
            "major",
            "lockFileMaintenance"
        ],
        "dependencyDashboardApproval": false,
        "stabilityDays": 0
    }]
}
